{
  "name": "eslint-config-iaguilarmartin",
  "version": "1.2.1",
  "description": "My personal ESLint configuration",
  "main": "index.js",
  "scripts": {
    "eslint": "npm run eslint:base && npm run eslint:react && npm run eslint:jest",
    "eslint:base": "eslint -c ./configs/.eslintrc .",
    "eslint:react": "eslint -c ./configs/.react.eslintrc .",
    "eslint:jest": "eslint -c ./configs/.jest.eslintrc .",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iaguilarmartin/eslint-config-iaguilarmartin.git"
  },
  "author": "Iván Aguilar Martín",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iaguilarmartin/eslint-config-iaguilarmartin/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide"
  ],
  "homepage": "https://github.com/iaguilarmartin/eslint-config-iaguilarmartin#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1"
  },
  "peerDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "babel-eslint": "^10.0.1"
  },
  "dependencies": {}
}
